{
	"name": "stelo",
	"author": "SonodaKazuto <https://github.com/SonodaKazuto>",
	"homepage": "sonodakazuto.stelo37710.moe",
	"license": "MIT",
	"description": "My personal website build from scratch",
	"repository": {
		"type": "git",
		"url": "https://github.com/SonodaKazuto/stelo"
	},
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"hook": "husky install",
		"dev": "vite",
		"build": "vue-tsc --noEmit && vite build",
		"preview": "vite preview",
		"format": "prettier --write .",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx,.vue",
		"lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.vue --fix",
		"commit": "cz",
		"release": "standard-version",
		"release:minor": "standard-version --release-as minor",
		"release:patch": "standard-version --release-as patch",
		"release:major": "standard-version --release-as major"
	},
	"config": {
		"commitizen": {
			"path": "@commitlint/cz-commitlint"
		}
	},
	"dependencies": {
		"vue": "^3.2.37"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-conventional": "^17.0.3",
		"@commitlint/cz-commitlint": "^17.0.3",
		"@iconify/json": "^2.1.77",
		"@ryansonshine/commitizen": "^4.2.8",
		"@types/markdown-it": "^12.2.3",
		"@types/node": "^18.0.3",
		"@typescript-eslint/eslint-plugin": "^5.30.6",
		"@typescript-eslint/parser": "^5.30.6",
		"@unocss/preset-attributify": "^0.44.1",
		"@unocss/preset-icons": "^0.44.1",
		"@unocss/preset-uno": "^0.44.1",
		"@unocss/vite": "^0.44.1",
		"@vitejs/plugin-vue": "^3.0.0",
		"@vue/eslint-config-prettier": "^7.0.0",
		"@vue/eslint-config-typescript": "^11.0.0",
		"eslint": "^8.19.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier-vue": "^4.2.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"eslint-plugin-vue": "^9.2.0",
		"happy-dom": "^6.0.3",
		"husky": "^8.0.1",
		"inquirer": "8.2.4",
		"lint-staged": "^13.0.3",
		"markdown-it": "^13.0.1",
		"markdown-it-anchor": "^8.6.4",
		"markdown-it-prism": "^2.2.4",
		"prettier": "2.7.1",
		"prettier-plugin-sh": "^0.12.6",
		"prismjs": "^1.28.0",
		"standard-version": "^9.5.0",
		"typescript": "^4.7.4",
		"vite": "^3.0.0",
		"vite-plugin-eslint": "^1.6.1",
		"vite-plugin-md": "^0.18.1",
		"vite-plugin-progress": "^0.0.3",
		"vite-plugin-theme": "^0.8.6",
		"vue-eslint-parser": "^9.0.3",
		"vue-tsc": "^0.38.5"
	}
}
